{
    "name": "react-thunk",
    "version": "1.0.0",
    "author": "Robert",
    "private": true,
    "description": "A React application using Thunk middleware.",
    "keywords": [
        "react",
        "redux",
        "redux-thunk"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/primalvinyl/react-thunk"
    },
    "bugs": "https://github.com/primalvinyl/react-thunk/issues",
    "dependencies": {
        "@reduxjs/toolkit": "^1.5.1",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.3.2",
        "@testing-library/user-event": "^7.1.2",
        "formik": "^2.1.5",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-redux": "^7.2.1",
        "react-router-dom": "^5.2.0",
        "react-scripts": "3.4.3",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0",
        "yup": "^0.29.3"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
